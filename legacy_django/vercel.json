{
    "builds": [{
        "src": "university_portal/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "university_portal/wsgi.py"
        }
    ]
}
